<launch>
 <rosparam>
  system_mapping:
  -
    pair: [simulator/consumer/manipulator, world_model/simulator/manipulator]
  -
    pair: [simulator/cup1, world_model/simulator/cup1]
  -
    pair: [simulator/cup2, world_model/simulator/cup2]
  -
    pair: [simulator/cup3, world_model/simulator/cup3]
  -
    pair: [simulator/cup4, world_model/simulator/cup4]
  -
    pair: [simulator/cup5, world_model/simulator/cup5]
  -
    pair: [simulator/cup6, world_model/simulator/cup6]
  -
    pair: [simulator/cup7, world_model/simulator/cup7]
  -
    pair: [simulator/cup8, world_model/simulator/cup8]
  -
    pair: [simulator/cup9, world_model/simulator/cup9]
  -
    pair: [simulator/cup10, world_model/simulator/cup10]
  -
    pair: [simulator/cup11, world_model/simulator/cup11]

  consumer: simulator/consumer
  </rosparam>

  <!-- k cups -->
  <rosparam command="load" ns="world_model/simulator/subsystems/cup1" file="$(find manipulation)/input/simulation/plants/cups.yaml"/>
  <rosparam command="load" ns="world_model/simulator/subsystems/cup2" file="$(find manipulation)/input/simulation/plants/cups.yaml"/>
  <rosparam command="load" ns="world_model/simulator/subsystems/cup3" file="$(find manipulation)/input/simulation/plants/cups.yaml"/> 
  <rosparam command="load" ns="world_model/simulator/subsystems/cup4" file="$(find manipulation)/input/simulation/plants/cups.yaml"/> 
  <rosparam command="load" ns="world_model/simulator/subsystems/cup5" file="$(find manipulation)/input/simulation/plants/cups.yaml"/> 
  <rosparam command="load" ns="world_model/simulator/subsystems/cup6" file="$(find manipulation)/input/simulation/plants/cups.yaml"/>
  <rosparam command="load" ns="world_model/simulator/subsystems/cup7" file="$(find manipulation)/input/simulation/plants/cups.yaml"/>
  <rosparam command="load" ns="world_model/simulator/subsystems/cup8" file="$(find manipulation)/input/simulation/plants/cups.yaml"/>
  <rosparam command="load" ns="world_model/simulator/subsystems/cup9" file="$(find manipulation)/input/simulation/plants/cups.yaml"/>
  <rosparam command="load" ns="world_model/simulator/subsystems/cup10" file="$(find manipulation)/input/simulation/plants/cups.yaml"/>
  <rosparam command="load" ns="world_model/simulator/subsystems/cup11" file="$(find manipulation)/input/simulation/plants/cups.yaml"/>

</launch>