  <rosparam ns="planning/graph_query_template">
    type: manipulation_query
    smooth_paths: false
    search_mode: lazy_search
    goal:
      type: multiple_goal_states
      distance_metric:
        type: ann_distance_metric
  </rosparam>
  <rosparam ns="planning/motion_query_template">
    type: pose_based_mp_query
    search_mode: lazy_search
    goal:
      type: multiple_goal_states
      distance_metric:
        type: ann_distance_metric
  </rosparam>

  <rosparam ns="planning/graph_specification_template">
    type: pose_based_mp_specification
    validity_checker: 
      type: world_model_validity_checker
    sampler:
      type: uniform_sampler
      default_orientation: [0,1,0,1]
      # min: [.95,-.43,.59]
      # max: [1.38,.43,1.59]
      #BIN B
      # initial_sample: [1.35, 0, 1.47]
      # min: [0.85,-.14,1.34]
      # max: [1.40,0.14,1.58]
      #BIN E
      # initial_sample: [1.38, 0, 1.21]
      # min: [0.85,-.15,1.35]
      # max: [1.40,0.15,1.59]
      #FRONT OF SHELF
      initial_sample: [0.80, 0, 1.20]
      min: [0.70,-.14,1.30]
      max: [0.90,0.14,1.60]
    distance_metric:
      type: graph_distance_metric
      distance_function: l_infinite_norm
    local_planner:
      type: bvp_local_planner
    stopping_criteria:
      elements:
        criterion1:
          type: iteration_criterion
          condition: 100
    heuristic_search:
      type: constraints_astar_search
  </rosparam>

  <rosparam ns="planning/task_planner/">
    type: apc_tp
    stat_file: apc_stats.txt
    specification:
      type: apc_driver_stress_specification
      sampler:
        type: uniform_sampler
    left_context_name: "left_full_arm"
    right_context_name: "right_full_arm"

    gripper_context_names: ["left_full_arm" , "right_full_arm", "right_upper_vacuum_full_arm", "right_parallel_gripper_full_arm"]

    left_camera_context_name: "left_camera"
    right_camera_context_name: "right_camera"
    manipulation_task_planner_name: "manipulation_task_planner"
    smooth_paths: false
    planners:
      manipulation_task_planner:
        use_ee_dist_in_planners: false
        constraints: 
          type: null_constraints
        specification:
          type: manipulation_specification
          validity_checker: 
            type: manipulation_validity_checker
          sampler:
            type: uniform_sampler
          distance_metric:
            type: graph_distance_metric
          local_planner:
            type: bvp_local_planner
          stopping_criteria:
            elements:
              criterion1:
                type: iteration_criterion
                condition: 100
        type: manipulation_tp
        num_candidate_connections: 30
        serialize_flag: false
        IK_steer_grasping: false
        skip_connectivity_check: true
        planners:
          left_arm_motion_planner:
            type: "prm_star"
            map_metric:
              type: ann_distance_metric
            end_effector_names: ["head_sponge"]
            sparsify: true
            stretch_factor: 3
            visualization_bodies: ["simulator/manipulator/head_sponge"]
            visualize_graph: true
            visualize: false
            visualize_solutions: false 
            deserialize_file: "left_arm_graph[8_k_top_middle_euclid].roadmap"
            # serialize_file: "sparse_left[100].roadmap"
            # deserialize_folder: "left_arm_rrg"
            space_name: "left_arm_planning"
            planning_context_name: "left_arm"
            query:
              template: motion_query_template
              lazy_astar_iterations: 100
            specification:
              template: graph_specification_template
              sampler:
                manipulation_context: left_full_arm
          right_arm_motion_planner:
            type: "prm_star"
            map_metric:
              type: ann_distance_metric
            end_effector_names: ["front_vacuum"]
            sparsify: false
            stretch_factor: 3
            visualization_bodies: ["simulator/manipulator/upper_vacuum"]
            visualize_graph: true
            visualize_solutions: false 
            visualize: false
            deserialize_file: "right_arm_graph[8_k_top_middle_euclid].roadmap"
            # serialize_file: "r10.roadmap"
            # deserialize_folder: "right_arm_rrg"
            space_name: "right_arm_planning"
            planning_context_name: "right_arm"
            query:
              template: motion_query_template
              lazy_astar_iterations: 100
            specification:
              template: graph_specification_template
              sampler:
                manipulation_context: right_full_arm
          right_upper_vacuum_arm_motion_planner:
            type: "prm_star"
            map_metric:
              type: ann_distance_metric
            end_effector_names: ["upper_vacuum"]
            sparsify: false
            stretch_factor: 3
            visualization_bodies: ["simulator/manipulator/upper_vacuum"]
            visualize_graph: true
            visualize_solutions: false 
            visualize: false
            deserialize_file: "right_arm_graph[8_k_top_middle_euclid].roadmap"
            # serialize_file: "r10.roadmap"
            # deserialize_folder: "right_arm_rrg"
            space_name: "right_arm_planning"
            planning_context_name: "right_upper_vacuum_arm"
            query:
              template: motion_query_template
              lazy_astar_iterations: 100
            specification:
              template: graph_specification_template
              sampler:
                manipulation_context: right_upper_vacuum_full_arm
          right_parallel_gripper_arm_motion_planner:
            type: "prm_star"
            map_metric:
              type: ann_distance_metric
            end_effector_names: ["parallel_gripper"]
            sparsify: false
            stretch_factor: 3
            visualization_bodies: ["simulator/manipulator/upper_vacuum"]
            visualize_graph: true
            visualize_solutions: false 
            visualize: false
            deserialize_file: "right_arm_graph[8_k_top_middle_euclid].roadmap"
            # serialize_file: "r10.roadmap"
            # deserialize_folder: "right_arm_rrg"
            space_name: "right_arm_planning"
            planning_context_name: "right_parallel_gripper_arm"
            query:
              template: motion_query_template
              lazy_astar_iterations: 100
            specification:
              template: graph_specification_template
              sampler:
                manipulation_context: right_parallel_gripper_full_arm
          left_rrt_motion_planner:
            type: rrt
            goal_bias_rate: 0.5
            visualization_bodies: ["simulator/manipulator/head_sponge"]
            visualize_tree: false
            visualize: false
            visualize_solutions: false 
            space_name: "left_arm_planning"
            planning_context_name: "left_arm"
            is_connection_planner: true
            query:
              type: motion_planning_query
              goal:
                type: multiple_goal_states
                distance_metric:
                  type: ann_distance_metric
            specification:
              template: tree_specification_template
              sampler:
                manipulation_context: left_full_arm 
          right_rrt_motion_planner:
            type: rrt
            goal_bias_rate: 0.5
            visualization_bodies: ["simulator/manipulator/upper_vacuum"]
            visualize_tree: false
            visualize: false
            visualize_solutions: false 
            space_name: "right_arm_planning"
            planning_context_name: "right_arm"
            is_connection_planner: true
            query:
              type: motion_planning_query
              goal:
                type: multiple_goal_states
                distance_metric:
                  type: ann_distance_metric
            specification:
              template: right_tree_specification_template
              sampler:
                manipulation_context: right_full_arm 
          right_upper_vacuum_rrt_motion_planner:
            type: rrt
            goal_bias_rate: 0.5
            visualization_bodies: ["simulator/manipulator/upper_vacuum"]
            visualize_tree: false
            visualize: false
            visualize_solutions: false 
            space_name: "right_arm_planning"
            planning_context_name: "right_upper_vacuum_arm"
            is_connection_planner: true
            query:
              type: motion_planning_query
              goal:
                type: multiple_goal_states
                distance_metric:
                  type: ann_distance_metric
            specification:
              template: right_tree_specification_template
              sampler:
                manipulation_context: right_upper_vacuum_full_arm 
          right_parallel_gripper_rrt_motion_planner:
            type: rrt
            goal_bias_rate: 0.5
            visualization_bodies: ["simulator/manipulator/upper_vacuum"]
            visualize_tree: false
            visualize: false
            visualize_solutions: false 
            space_name: "right_arm_planning"
            planning_context_name: "right_parallel_gripper_arm"
            is_connection_planner: true
            query:
              type: motion_planning_query
              goal:
                type: multiple_goal_states
                distance_metric:
                  type: ann_distance_metric
            specification:
              template: right_tree_specification_template
              sampler:
                manipulation_context: right_parallel_gripper_full_arm 
          grasping_planner:
            type: grasping_planner
            specification:
              type: grasping_specification
              validity_checker:
                type: world_model_validity_checker
            grasp_generators:
              left_full_arm:
                template: unigripper_grasp_generator
              right_full_arm:
                template: unigripper_grasp_generator
              right_upper_vacuum_full_arm:
                template: unigripper_grasp_generator
              right_parallel_gripper_full_arm:
                template: unigripper_grasp_generator
  </rosparam>

  <rosparam ns="planning/unigripper_grasp_generator">
    type: unigripper_grasp_generator
    default_ee_normal: [0,0,1]
    grasp_entire_mesh: false
    save_grasps_to_file: true
    restricted_normals:
    -
      normal: [0,0,-1]
    -
      normal: [1,0,0]
    preferred_normals:
    -
      normal: [0,0,1]
    -
      normal: [-1,0,0]
    manual_grasp_mode: 2
    manual_release_mode: 1
    nr_grasps_to_sample: 60
    # manual_angle_increment: 15.0
  </rosparam>

  <rosparam ns="planning/tree_specification_template">
    type: motion_planning_specification
    validity_checker: 
      type: world_model_validity_checker
    sampler:
      type: uniform_sampler
    distance_metric:
      type: graph_distance_metric
      distance_function: l_infinite_norm
    local_planner:
      type: bvp_local_planner
      max_prop_length: 0.05
    stopping_criteria:
      criteria_check_type: 2
      elements:
        criterion1:
          type: iteration_criterion
          condition: 5000
        criterion2:
          type: goal_criterion
          criterion_type: goal
  </rosparam>
  <rosparam ns="planning/right_tree_specification_template">
