<launch>
  <rosparam command="load" file="$(find prx_input)/templates/applications/empty_application.yaml" />
  
  <rosparam command="load" file="$(find prx_input)/templates/simulators/collision_stop_simulator.yaml" />
  
  <rosparam>
    interval: 0.05
  </rosparam>
  
  <rosparam ns="simulator">
  simulation_step: 0.05
  </rosparam>
  
  <rosparam command="load" ns="manual_controllers">
    system_paths: [simulator/manual]
  </rosparam>
  
  <rosparam command="load" ns="simulator/subsystems/manual" file="$(find prx_input)/templates/controllers/consumer.yaml"/>
  
  <rosparam command="load" ns="baxter" file="$(find baxter)/input/urdf/baxter.yaml"/>
  <rosparam command="load" ns="simulator/subsystems/manual/subsystems/manipulator">
    template: "baxter"
    initial_state: [-0.810000,0.000000,0.000000,1.260000,0.000000,0.000000,0.000000,0.000000]
    initial_control: [0,0,0,0,0,0,0,0]
    print: false
  </rosparam>
  
  <rosparam command="load" ns="simulator/obstacles" file="$(find baxter)/input/simulation/baxter_torso.yaml" />
  <rosparam command="load" ns="simulator/obstacles" file="$(find parallel_search)/input/obstacles/shelf.yaml" />
</launch>