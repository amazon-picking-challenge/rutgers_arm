<launch>

  <rosparam ns="planning2">
    state_mapping:
      - 
        pair: [simulator/consumer02/vo_controller02/disk02, world_model/simulator/disk]
    control_mapping:
      - 
        pair: [simulator/consumer02/vo_controller02, world_model/simulator/disk]
      - 
        pair: [simulator/consumer02/vo_controller02/disk02, world_model/simulator/disk]
    consumer_mapping:
      - 
        pair: [simulator/consumer02, space1]
    consumer: simulator/consumer02
    my_plant: simulator/consumer02/vo_controller02/disk02
    planning_duration: 0.5
    visualize: false
    rxandom_seed: 1495402825
    simulation_node_name: generic_name
  </rosparam>

  <include ns="planning2" file="$(find GTA)/input/experiments/test/4_experiment/planning/world_models/world_model.launch"/>
  <include ns="planning2" file="$(find GTA)/input/experiments/test/4_experiment/planning/planner_structure/planner_structure.launch"/>
  <node name="planning2"  pkg="prx_planning" type="prx_planning" required="false" launch-prefix="" output="log" args="planning2"/>

</launch>

