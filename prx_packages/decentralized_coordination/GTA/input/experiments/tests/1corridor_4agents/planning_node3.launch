<!-- This file has been tested on PRACSYS revision  -->

<launch>
        
  <!-- Define planning parameters -->           
  <include ns="planning3" file="$(find GTA)/input/experiments/tests/1corridor_4agents/planning/world_models/main_example_1.launch"/>
  <include ns="planning3" file="$(find GTA)/input/experiments/tests/1corridor_4agents/planning/planner_structures/main_example_1.launch"/>
        
  <rosparam ns="planning3">
    consumer_mapping:
      - 
        pair: [simulator/consumer3, space1]
    my_plant: simulator/consumer3/vo_controller3/disk3
    state_mapping:
      - 
        pair: [simulator/consumer3/vo_controller3/disk3, world_model/simulator/disk]
    control_mapping:
      - 
        pair: [simulator/consumer3/vo_controller3, world_model/simulator/disk]
      - 
        pair: [simulator/consumer3/vo_controller3/disk3, world_model/simulator/disk]
    consumer: simulator/consumer3
  </rosparam>
            
  <!-- Define the planning node -->
  <machine name="arianne" address="arianne.cs.rutgers.edu" env-loader="~/remote_ros/remote_robb.sh" user="pracsys" password="r0b0t1cs"/>
  <node machine="arianne" name="planning3" pkg="prx_planning" type="prx_planning"
        required="true" launch-prefix="" output="screen" args="planning3"/>


</launch>