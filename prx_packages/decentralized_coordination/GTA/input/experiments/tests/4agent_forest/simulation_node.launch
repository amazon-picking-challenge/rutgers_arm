<!-- This file has been tested on PRACSYS revision  -->

<launch>
        

  <!-- Define simulation parameters -->      
  <include file="$(find GTA)/input/experiments/tests/4agent_forest/simulation/main_example.launch"/>
           
  <!-- Define the simulation node -->
  <node name="simulation" pkg="prx_simulation" type="prx_simulation"
        required="true" launch-prefix="" output="screen" />


  <rosparam ns="simulation" >
        random_seed: 123456789
  </rosparam>
        
  <include file="$(find prx_input)/templates/visualization/visualization.launch"/>
        
  <rosparam ns="visualization/viewer/scene" >
   line_thickness: 3
  </rosparam>

</launch>