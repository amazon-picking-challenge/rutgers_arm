<!-- This file has been tested on PRACSYS revision  -->

<launch>
        
  <!-- Define planning parameters -->           
  <include ns="planning3" file="$(find GTA)/input/experiments/tests/4agent_forest/planning/world_models/main_example_3.launch"/>
  <include ns="planning3" file="$(find GTA)/input/experiments/tests/4agent_forest/planning/planner_structures/main_example_3.launch"/>
  <rosparam ns="planning3">
    consumer_mapping:
      - 
        pair: [simulator/consumer3, space1]
    my_plant: simulator/consumer3/vo_controller3/disk3
    random_seed: 123456789
  </rosparam>
  <!-- Define the planning node -->
  <node name="planning3" pkg="prx_planning" type="prx_planning"
        required="true" launch-prefix="" output="screen" args="planning3"/>   


</launch>