<!-- This file has been tested on PRACSYS revision  -->

<launch>
        
  <!-- Define planning parameters -->           
  <include ns="planning2" file="$(find GTA)/input/experiments/tests/forest2/planning/world_models/main_example_2.launch"/>
  <include ns="planning2" file="$(find GTA)/input/experiments/tests/forest2/planning/planner_structures/main_example_2.launch"/>
  <rosparam ns="planning2">
    consumer_mapping:
      - 
        pair: [simulator/consumer2, space1]
    my_plant: simulator/consumer2/vo_controller2/disk2
  </rosparam>
  <!-- Define the planning node -->
  <node name="planning2" pkg="prx_planning" type="prx_planning"
        required="true" launch-prefix="" output="screen" args="planning2"/>   


</launch>